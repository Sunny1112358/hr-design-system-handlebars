{{~#each this.logicItems}}
    {{~#if this.isSsi~}}        
        {{#if (isStorybook)}}  
            {{> components/Teaser/teaser_logic.ssi.hbs }} 
        {{else}}
            {{{this.ssi}}}
        {{/if}} 
    {{~else}}
        {{~#with this.includeModel~}}
            {{~#if (isStorybook)~}}
                {{~> components/base/loadDynamic templatePath=../includePath ~}}
            {{~else~}}
                {{~> (lookup ../. 'includePath')~}}
            {{~/if~}}
        {{~/with~}}
    {{~/if~}}
{{/each~}}